<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrapcss")
    @RenderSection("head",false)
</head>

 <div class="container-fluid">
 <div class="navbar navbar-static-top">
  <div class="page-header">
  <h1 class="text-success">My Project Test <small>sean enriquez</small></h1>
</div>
</div>
 <div class="row-fluid">
 <div class="span3">
 <div class="well sidebar-nav">
 <ul class="nav nav-list">
 <li class="nav-header">Menu</li>
 <li> @Html.ActionLink("Home","Index","MyTemplate")</li>
 <li> @Html.ActionLink("About Us","AboutUs","MyTemplate")</li>
 <li> @Html.ActionLink("News","News","MyTemplate")</li>

 </ul>
 </div>
 </div>
 <div class="span9">
 @RenderSection("featured", required: false)
 <div class="row-fluid">
 @RenderBody()
 </div>
 </div>
 </div>
 </div>
 @Scripts.Render("~/bundles/jquery")
 @Scripts.Render("~/bundles/bootstrapjs")
</body>
</html>
